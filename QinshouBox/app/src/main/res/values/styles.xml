<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="SafetyPaddingStyle">
        <item name="android:paddingLeft">@dimen/px30</item>
        <item name="android:paddingStart" tools:targetApi="jelly_bean_mr1">@dimen/px30</item>
        <item name="android:paddingRight">@dimen/px30</item>
        <item name="android:paddingEnd" tools:targetApi="jelly_bean_mr1">@dimen/px30</item>
        <item name="android:paddingTop">@dimen/px30</item>
        <item name="android:paddingBottom">@dimen/px30</item>
    </style>
</resources>
